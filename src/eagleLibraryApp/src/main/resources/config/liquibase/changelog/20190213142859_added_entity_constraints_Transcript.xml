<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Transcript.
    -->
    <changeSet id="20190213142859-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="id"
                                 baseTableName="transcript"
                                 constraintName="fk_transcript_page_id"
                                 referencedColumnNames="id"
                                 referencedTableName="page"/>

        <addForeignKeyConstraint baseColumnNames="transcript_id"
                                 baseTableName="user_trans_link"
                                 constraintName="fk_transcript_user_transcript_id"
                                 referencedColumnNames="id"
                                 referencedTableName="transcript"/>
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="user_trans_link"
                                 constraintName="fk_transcript_user_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>
        
    </changeSet>
</databaseChangeLog>
