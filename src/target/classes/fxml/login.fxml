<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.univaq.eaglelibrary.view.LoginControllerGUI">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="login" layoutX="274.0" layoutY="224.0" mnemonicParsing="false" onAction="#login" text="Login" />
            <Button fx:id="registration" layoutX="257.0" layoutY="308.0" mnemonicParsing="false" onAction="#registration" text="Registrazione" />
            <Label layoutX="230.0" layoutY="282.0" text="se non sei ancora registrato:" />
            <Label layoutX="160.0" layoutY="99.0" text="Username" />
            <Label layoutX="162.0" layoutY="157.0" text="Password" />
            <TextField fx:id="username" layoutX="274.0" layoutY="95.0" />
            <TextField fx:id="password" layoutX="274.0" layoutY="153.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
